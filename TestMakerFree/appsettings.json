{
  "ConnectionStrings": {
    "SqlDbConnection": "Server=(localdb)\\MSSQLLocalDB;Database=test-maker-free;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "None"
      }
    },
