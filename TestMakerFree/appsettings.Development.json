{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "None"
      }
    },
    "StaticFiles": {
      "Headers": {
        "Cache-Control": "no-cache, no-store",
        "Pragma": "no-cache",
        "Expires": "-1"
      }
    }
  }
}